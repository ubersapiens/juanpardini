    <script src="https://code.jquery.com/jquery-migrate-1.2.1.js"></script>
    <script src="/assets/raphael.2.1.0.min.js"></script>
    <script src="/assets/justgage.1.0.1.min.js"></script>
    <%= csrf_meta_tag %>
    <%= yield(:head) %>
